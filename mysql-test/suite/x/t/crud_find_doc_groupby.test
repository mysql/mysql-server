--write_file $MYSQL_TMP_DIR/crud_find_doc_groupby.xpl
-->quiet

-->stmtadmin create_collection	{"schema":"xtest","name":"xcoll"}
-->recvresult

INSERT INTO xtest.xcoll (doc) VALUES
('{"_id": 1, "age": 1, "name": "foo"}'),
('{"_id": 2, "age": 2, "name": "bar"}'),
('{"_id": 3, "age": 3, "name": "baz", "date": {"day": 20, "month": "Apr"}} '),
('{"_id": 4, "age": 7, "name": "foo"}'),
('{"_id": 5, "age": 17, "name": "buz"}');

-->echo group by name
Mysqlx.Crud.Find {
  collection { name: "xcoll" schema: "xtest" }
  data_model: DOCUMENT
  projection {
    alias: "name"
    source {
      type: IDENT
      identifier {
        document_path {
          type: MEMBER
          value: "name"
        }
      }
    }
  }
  grouping {
    type: IDENT
    identifier {
      document_path {
        type: MEMBER
        value: "name"
      }
    }
  }
}
-->recvresult

-->echo group by name and count
Mysqlx.Crud.Find {
  collection { name: "xcoll" schema: "xtest" }
  data_model: DOCUMENT
  projection {
    alias: "name"
    source {
      type: IDENT
      identifier {
        document_path {
          type: MEMBER
          value: "name"
        }
      }
    }
  }
  projection {
    alias: "count"
    source {
      type: FUNC_CALL
      function_call {
        name {
          name: "count"
        }
        param {
          type: OPERATOR operator {
            name: "*"
          }
        }
      }
    }
  }
  grouping {
      type: IDENT
      identifier {
        document_path {
          type: MEMBER
          value: "name"
        }
      }
  }
}
-->recvresult

