--disable_warnings
let $ENGINE=`select variable_value from information_schema.global_variables where variable_name='DEFAULT_STORAGE_ENGINE'`;
--enable_warnings

--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings
CREATE TABLE t1(c1 TINYINT NOT NULL, c2 TINYINT NOT NULL, c3 TINYINT NOT NULL);
INSERT INTO t1 VALUES(1,1,1),(1,1,2),(1,2,1),(1,2,2),(2,1,1);
ALTER TABLE t1 ORDER BY c1,c2,c3;
SELECT * FROM t1;
ALTER TABLE t1 ORDER BY c3,c2,c1;
SELECT * FROM t1;
ALTER TABLE t1 ORDER BY c3,c2,c1 DESC;
SELECT * FROM t1;
--replace_result $ENGINE ENGINE " PAGE_CHECKSUM=0" ""
 SHOW CREATE TABLE t1; DROP TABLE t1; SHOW TABLES;
