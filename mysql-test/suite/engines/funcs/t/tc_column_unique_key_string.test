########################################
##### WL#7554 Force schema `test` to character set latin1
##### This test should be migrated to utf8mb4
--disable_query_log
let $testdbcs79=`select default_character_set_name from information_schema.schemata where schema_name='test'`;
alter schema test default character set latin1;
--enable_query_log
########################################

--disable_warnings
let $ENGINE=`select variable_value from performance_schema.global_variables where variable_name='DEFAULT_STORAGE_ENGINE'`;
--enable_warnings

--disable_warnings
DROP TABLE IF EXISTS t9;
--enable_warnings
CREATE TABLE t9(c1 CHAR(10) NOT NULL UNIQUE KEY);
SHOW TABLES;
--replace_result $ENGINE ENGINE " PAGE_CHECKSUM=0" ""
SHOW CREATE TABLE t9; DROP TABLE t9; SHOW TABLES;
CREATE TABLE t9(c1 VARCHAR(10) NOT NULL UNIQUE KEY);
SHOW TABLES;
--replace_result $ENGINE ENGINE " PAGE_CHECKSUM=0" ""
SHOW CREATE TABLE t9; DROP TABLE t9; SHOW TABLES;
CREATE TABLE t9(c1 BINARY(10) NOT NULL UNIQUE KEY);
SHOW TABLES;
--replace_result $ENGINE ENGINE " PAGE_CHECKSUM=0" ""
SHOW CREATE TABLE t9; DROP TABLE t9; SHOW TABLES;
CREATE TABLE t9(c1 VARBINARY(10) NOT NULL UNIQUE KEY);
SHOW TABLES;
--replace_result $ENGINE ENGINE " PAGE_CHECKSUM=0" ""
SHOW CREATE TABLE t9; DROP TABLE t9; SHOW TABLES;


########################################
##### WL#7554 set schema `test` back to what it was
--disable_query_log
--eval alter schema test default character set $testdbcs79;
--enable_query_log
########################################
