--disable_warnings
let $ENGINE=`select variable_value from performance_schema.global_variables where variable_name='DEFAULT_STORAGE_ENGINE'`;
--enable_warnings

--disable_warnings
DROP TABLE IF EXISTS m2;
--enable_warnings
CREATE TABLE m2(c1 CHAR(1) NULL, c2 CHAR(5) NOT NULL, c3 CHAR(255) NULL, c4 CHAR(4) NOT NULL PRIMARY KEY, c5 CHAR(30) NOT NULL UNIQUE KEY);
SHOW TABLES;
--replace_result $ENGINE ENGINE " PAGE_CHECKSUM=0" ""
SHOW CREATE TABLE m2; DROP TABLE m2; SHOW TABLES;
