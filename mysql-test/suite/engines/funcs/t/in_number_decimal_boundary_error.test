INSERT INTO t5 (c1) VALUES(999999);
SELECT COUNT(c1) AS total_rows FROM t5;
SELECT COUNT(c1) AS null_rows FROM t5 WHERE c1 IS NULL;
SELECT COUNT(c1) AS not_null_rows FROM t5 WHERE c1 IS NOT NULL;
