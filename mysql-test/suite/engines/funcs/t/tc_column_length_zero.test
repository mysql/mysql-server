--disable_warnings
let $ENGINE=`select variable_value from performance_schema.global_variables where variable_name='DEFAULT_STORAGE_ENGINE'`;
--enable_warnings

--disable_warnings
DROP TABLE IF EXISTS t6;
--enable_warnings
CREATE TABLE t6(c1 CHAR(0) NULL);
SHOW TABLES;
