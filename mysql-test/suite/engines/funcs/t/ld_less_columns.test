# This line and the line below is a temporary line for tests that do not need MyISAM in future, but need to be kept for tests that need it.
--source include/have_myisam.inc

--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings
CREATE TABLE t1 (c1 INTEGER NOT NULL PRIMARY KEY, c2 TEXT);
LOAD DATA LOCAL INFILE 'suite/engines/funcs/t/load_less_columns.inc' INTO TABLE t1 FIELDS TERMINATED BY ',' LINES TERMINATED BY '\n';
SELECT * FROM t1 ORDER BY c1;
TRUNCATE TABLE t1;
--copy_file suite/engines/funcs/t/load_less_columns.inc $MYSQLTEST_VARDIR/tmp/t1.dat
--disable_query_log
eval LOAD DATA LOCAL INFILE '$MYSQLTEST_VARDIR/tmp/t1.dat' INTO TABLE t1 FIELDS TERMINATED BY ',' LINES TERMINATED BY '\n';
--enable_query_log
SELECT * FROM t1 ORDER BY c1;
remove_file $MYSQLTEST_VARDIR/tmp/t1.dat;
DROP TABLE t1;

