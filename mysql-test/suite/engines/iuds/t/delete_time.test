#Deleting the table rows
--sorted_result
SELECT c1 FROM t1 WHERE c1='00:00:07';
DELETE FROM t1 WHERE c1='00:00:07';
--sorted_result
SELECT c1 FROM t1;
--sorted_result
SELECT c1 FROM t2 WHERE c1='-838:59:59' AND c2='-838:59:59';
