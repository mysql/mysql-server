# Clean up
#
server2
SET @@GLOBAL.DEBUG= @debug_save_s2;
include/stop_group_replication.inc
server3
SET @@GLOBAL.DEBUG= @debug_save_s3;
include/stop_group_replication.inc
server4
SET @@GLOBAL.DEBUG= @debug_save_s4;
include/stop_group_replication.inc
