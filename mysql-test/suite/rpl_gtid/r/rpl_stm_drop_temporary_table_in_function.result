CREATE FUNCTION create_temp_table() RETURNS INT DETERMINISTIC
BEGIN
CREATE TEMPORARY TABLE temp1 (i INT);
return 1;
END//
CREATE FUNCTION drop_temp_table() RETURNS INT DETERMINISTIC
BEGIN
DROP TEMPORARY TABLE temp1;
return 2;
END//
