       JSON_VALUE(JSON_OBJECT('a', NULL), '$.a' RETURNING SIGNED ERROR ON ERROR),
       JSON_VALUE(JSON_OBJECT('a', NULL), '$.a' RETURNING SIGNED);

# As CHAR:
