SHOW SESSION STATUS LIKE 'Handler_update%';
disable_result_log;
SELECT t1, JSON_ARRAYAGG(t2) FROM t join p on t.t1=p.p1 group by t1;
enable_result_log;
SHOW SESSION STATUS LIKE 'Handler_update%';

