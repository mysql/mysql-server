KEY ((CAST(doc->'$[*].name' AS CHAR(3) ARRAY))),
KEY ((CAST(doc->'$[*].date' AS DATE ARRAY))));
INSERT INTO t VALUES (
'[{"name": "abc", "date": "2021-01-01"}, {"name": "def", "date": "2021-01-02"},
  {"name": "ghi", "date": "2021-01-03"}, {"name": "jkl", "date": "2021-01-04"}]'
);
