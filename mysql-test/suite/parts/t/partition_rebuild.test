########################################
##### WL#7554 Force schema `test` to character set latin1
##### This test should be migrated to utf8mb4
--disable_query_log
let $testdbcs351=`select default_character_set_name from information_schema.schemata where schema_name='test'`;
alter schema default character set latin1;
use test;
--enable_query_log
########################################

--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings
 CREATE TABLE t1 ( c1 INTEGER NOT NULL PRIMARY KEY, c2 CHAR(10)) PARTITION BY RANGE (c1) (
    PARTITION p0 VALUES LESS THAN (100),
    PARTITION p1 VALUES LESS THAN (200),
    PARTITION p2 VALUES LESS THAN (300),
    PARTITION p3 VALUES LESS THAN (400),
    PARTITION p4 VALUES LESS THAN (500),
    PARTITION p5 VALUES LESS THAN MAXVALUE);
SHOW TABLES; 
INSERT INTO t1 VALUES(0,'abc'),(100,'abc'),(200,'abc'),(300,'abc'),(400,'abc');
SELECT * FROM t1 ORDER BY c1;
 SHOW CREATE TABLE t1;
ALTER TABLE t1 REBUILD PARTITION p1,p2;
SELECT * FROM t1 ORDER BY c1;
 SHOW CREATE TABLE t1;
 DROP TABLE t1; SHOW TABLES;


########################################
##### WL#7554 set schema `test` back to what it was
--disable_query_log
--eval alter schema test default character set $testdbcs351;
--enable_query_log
########################################
