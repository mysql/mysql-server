backup: t1#p#abc#sp#sub_abc0
backup: t1#p#abc#sp#sub_abc1
backup: t1#p#def#sp#sub_def0
backup: t1#p#def#sp#sub_def1
unlock tables;
drop table t1;
create table t1 (f1 int ,f2 int) partition by range (f1)
subpartition by key (f2)
