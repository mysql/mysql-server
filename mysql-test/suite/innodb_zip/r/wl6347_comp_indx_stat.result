SET GLOBAL innodb_cmp_per_index_enabled=0;
SET GLOBAL innodb_cmp_per_index_enabled=1;
# set the flags
SET GLOBAL innodb_file_per_table=on;
SET GLOBAL innodb_compression_level=9;
