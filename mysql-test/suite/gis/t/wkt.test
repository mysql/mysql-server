--error ER_LATITUDE_OUT_OF_RANGE
SELECT ST_POINTFROMTEXT('POINT(90.0000001 10)',32100000);
--error ER_LONGITUDE_OUT_OF_RANGE
SELECT ST_POINTFROMTEXT('POINT(10 180.0000001)',32100000);

