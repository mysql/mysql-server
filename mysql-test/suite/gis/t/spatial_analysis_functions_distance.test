                   ST_GEOMFROMTEXT('POINT(90 0)', 4326)), 3) AS d;
SELECT ROUND(ST_DISTANCE(ST_GEOMFROMTEXT('POINT(0 0)', 4326),
                   ST_GEOMFROMTEXT('MULTIPOINT((90 0))', 4326)), 3) AS d;
SELECT ROUND(ST_DISTANCE(ST_GEOMFROMTEXT('MULTIPOINT((0 0))', 4326),
                   ST_GEOMFROMTEXT('POINT(90 0)', 4326)), 3) AS d;

