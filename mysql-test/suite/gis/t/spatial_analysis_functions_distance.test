DO ST_DISTANCE(
  @pt_long_below,
  ST_SRID(ST_SWAPXY(ST_GEOMFROMTEXT('MULTIPOINT((0 0))')), 4326)
) AS d;
# Just within range
DO ST_DISTANCE(
