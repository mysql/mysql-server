# This test tests the spatial reference system framework:
#
#  - CREATE SPATIAL REFERENCE SYSTEM
#  - DROP SPATIAL REFERENCE SYSTEM
#  - SRS WKT parser
#  - Semantic checking of projection parameters

# Store the number of SRSs. Compare at the end to detect if we create an SRS and
# forget to drop it afterwards.
SET @number_of_srss =
  (SELECT COUNT(*) FROM INFORMATION_SCHEMA.ST_SPATIAL_REFERENCE_SYSTEMS);

# Use all defined SRSs and thereby verify that they are parseable and
# contain all mandatory parameters. We don't care about the result.

--disable_result_log
SELECT
  ST_DISTANCE(
    ST_GEOMFROMTEXT('POINT(0 0)', srs_id),
    ST_GEOMFROMTEXT('POINT(0 0)', srs_id)
  )
  FROM information_schema.ST_SPATIAL_REFERENCE_SYSTEMS;
--enable_result_log

# Check that I_S contains the correct information for at least one SRS
SELECT * FROM information_schema.ST_SPATIAL_REFERENCE_SYSTEMS WHERE srs_id=4326;

# The following tests verify that users without the SUPER privilege are not
# allowed to create and drop SRSs.

# Save the initial number of concurrent sessions
--source include/count_sessions.inc

CREATE USER srs_test@localhost;

# Create an SRS to drop.
CREATE SPATIAL REFERENCE SYSTEM 1000000000
NAME 'foo'
DEFINITION 'GEOGCS["WGS 84",DATUM["World Geodetic System 1984",SPHEROID["WGS 84",6378137,298.257223563,AUTHORITY["EPSG","7030"]],AUTHORITY["EPSG","6326"]],PRIMEM["Greenwich",0,AUTHORITY["EPSG","8901"]],UNIT["degree",0.017453292519943278,AUTHORITY["EPSG","9122"]],AXIS["Lat",NORTH],AXIS["Lon",EAST],AUTHORITY["EPSG","4326"]]';

--connect(nonsuper,localhost,srs_test)
--connection nonsuper

--echo # CREATE as non-SUPER (should raise error).
--error ER_CMD_NEED_SUPER
CREATE SPATIAL REFERENCE SYSTEM 1000000001
NAME 'foo'
DEFINITION 'GEOGCS["WGS 84",DATUM["World Geodetic System 1984",SPHEROID["WGS 84",6378137,298.257223563,AUTHORITY["EPSG","7030"]],AUTHORITY["EPSG","6326"]],PRIMEM["Greenwich",0,AUTHORITY["EPSG","8901"]],UNIT["degree",0.017453292519943278,AUTHORITY["EPSG","9122"]],AXIS["Lat",NORTH],AXIS["Lon",EAST],AUTHORITY["EPSG","4326"]]';

--echo # CREATE OR REPLACE on existing SRS as non-SUPER (should raise error).
--error ER_CMD_NEED_SUPER
CREATE OR REPLACE SPATIAL REFERENCE SYSTEM 1000000000
NAME 'foo'
DEFINITION 'GEOGCS["WGS 84",DATUM["World Geodetic System 1984",SPHEROID["WGS 84",6378137,298.257223563,AUTHORITY["EPSG","7030"]],AUTHORITY["EPSG","6326"]],PRIMEM["Greenwich",0,AUTHORITY["EPSG","8901"]],UNIT["degree",0.017453292519943278,AUTHORITY["EPSG","9122"]],AXIS["Lat",NORTH],AXIS["Lon",EAST],AUTHORITY["EPSG","4326"]]';

--echo # CREATE SRS IF NOT EXISTS on existing SRS as non-SUPER (should raise error).
--error ER_CMD_NEED_SUPER
CREATE SPATIAL REFERENCE SYSTEM IF NOT EXISTS 1000000000
NAME 'foo'
DEFINITION 'GEOGCS["WGS 84",DATUM["World Geodetic System 1984",SPHEROID["WGS 84",6378137,298.257223563,AUTHORITY["EPSG","7030"]],AUTHORITY["EPSG","6326"]],PRIMEM["Greenwich",0,AUTHORITY["EPSG","8901"]],UNIT["degree",0.017453292519943278,AUTHORITY["EPSG","9122"]],AXIS["Lat",NORTH],AXIS["Lon",EAST],AUTHORITY["EPSG","4326"]]';

--echo # DROP on existing SRS as non-SUPER(should raise error).
--error ER_CMD_NEED_SUPER
DROP SPATIAL REFERENCE SYSTEM 1000000000;

--echo # DROP on non-existing SRS as non-SUPER (should raise error).
--error ER_CMD_NEED_SUPER
DROP SPATIAL REFERENCE SYSTEM 1000000000;

--connection default
--disconnect nonsuper

# Wait until all disconnects are completed
--source include/wait_until_count_sessions.inc

DROP SPATIAL REFERENCE SYSTEM 1000000000;
DROP USER srs_test@localhost;

