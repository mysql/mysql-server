--echo # Check Cursor with Geometry 
DELIMITER |;
CREATE PROCEDURE geom_cursor()
BEGIN
    DECLARE v GEOMETRY;
    DECLARE c CURSOR FOR SELECT c8 FROM tab2;
    OPEN c;
    FETCH c INTO v;
    CLOSE c;
    SELECT ST_ASTEXT(v);
END|
DELIMITER ;|

