# bug#58280
create table t1(vc varchar(16), i int, vc2 varchar(1024)
  , PRIMARY KEY(vc,vc2) USING HASH
  , KEY i1(i)
  , KEY i2(vc)
 )  ENGINE=ndbcluster
    CHARACTER SET latin1
    COMMENT="NDB_TABLE=PARTITION_BALANCE=FOR_RP_BY_LDM";

