###
### insert PK vs PK
###
create table t1 (
  a int not null,
  b int not null,
  c int not null,
  d int not null,
  e int not null,
  f int not null,
  primary key (a),
  unique(c) using hash,
  index(e)) engine = ndb;

alter table t1 add constraint fkname
foreign key (a) references t1(a) on delete restrict on update restrict;

insert into t1 values (1,1,1,1,1,1);

alter table t1 drop foreign key fkname;
delete from t1;

