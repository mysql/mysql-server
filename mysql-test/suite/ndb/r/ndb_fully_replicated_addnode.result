CREATE TABLE t1 (
a char(10) CHARSET latin1 NOT NULL,
e varbinary(3000) not null,
b int,
c blob,
d varbinary(26000),
primary key(e,a),
unique key (b)
) comment='NDB_TABLE=FULLY_REPLICATED=1' ENGINE=ndbcluster;
