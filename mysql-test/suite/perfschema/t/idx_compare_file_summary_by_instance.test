#
# Verify that query results are the same with and without indexes.
#
--source include/have_debug_sync.inc

let $table = "file_summary_by_instance";
let $target_name = wait/io/file/sql/ERRMSG;

--echo TABLE: $table
--echo INDEXES: PK (OBJECT_INSTANCE_BEGIN), KEY (FILE_NAME), KEY (EVENT_NAME)

--source ../include/idx_compare_setup.inc

--disable_query_log
# Get an object_instance_begin and file name
eval SELECT object_instance_begin, file_name INTO @object_instance_begin, @file_name
  FROM performance_schema.file_summary_by_instance
  WHERE event_name = '$target_name' LIMIT 1;
--enable_query_log

--echo
--echo ## Test OBJECT_INSTANCE_BEGIN
let $key_group = 12;
--source ../include/idx_compare_load.inc

--echo
--echo ## Test EVENT_NAME
let $key_group = 13;
let $name_column = EVENT_NAME;
let $target_name = wait/io/file/sql/ERRMSG;

--source ../include/idx_compare_load.inc

--echo
--echo ## Test FILE_NAME
let $key_group = 13.1;
let $name_column = FILE_NAME;
SET @target_name = @file_name;

--source ../include/idx_compare_load.inc

--source ../include/idx_compare_cleanup.inc
