  from performance_schema.global_status;

--echo
--echo # STEP 1-9
--echo # EMPTY, NO WARNINGS (not available in 5.6 mode)
select *
  from performance_schema.status_by_thread;

--echo
--echo # STEP 1-10
insert into test.sanity
