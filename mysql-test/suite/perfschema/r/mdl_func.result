drop function if exists thread_id_name;
create function thread_id_name(THREAD_ID int)
returns varchar(16)
begin
if (THREAD_ID IS NULL)
then
return NULL;
end if;
if (THREAD_ID = @user1_tid)
then
return "USER1";
end if;
if (THREAD_ID = @user2_tid)
then
return "USER2";
end if;
return "OTHER";
end;
