create procedure proc1(a int, b int)
BEGIN
prepare st1 from 'SELECT SQRT(POW(?,2) + POW(?,2)) AS hypotenuse';
execute st1 using @a, @b;
END //
SET @a = 3;
SET @b = 4;
call proc1(@a, @b);
hypotenuse
