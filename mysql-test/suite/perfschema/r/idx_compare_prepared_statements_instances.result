TABLE: "prepared_statements_instances"
INDEXES: PK (OBJECT_INSTANCE_BEGIN), KEY (STATEMENT_ID), KEY (STATEMENT_NAME),
KEY (OBJECT_TYPE, OBJECT_SCHEMA, OBJECT_NAME), KEY (OWNER_THREAD_ID, OWNER_EVENT_ID)

