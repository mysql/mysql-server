--source suite/collations/include/unicode.inc
insert into unicode values (0x100001,'AA','Norwegian AA','Lu');
insert into unicode values (0x100002,'Aa','Norwegian Aa','Lt');
insert into unicode values (0x100003,'aa','Norwegian aa','Ll');
select hex(convert(uc using utf32)), hex(weight_string(convert(uc using utf8mb4) collate utf8mb4_nb_0900_ai_ci)), name from unicode where category in ('Lu','Ll','Lt','Lm') order by uc  collate utf8mb4_nb_0900_ai_ci, cp;
select hex(convert(uc using utf32)), hex(weight_string(convert(uc using utf8mb4) collate utf8mb4_nb_0900_as_cs)), name from unicode where category in ('Lu','Ll','Lt','Lm') order by uc  collate utf8mb4_nb_0900_as_cs, cp;
drop table if exists unicode;
