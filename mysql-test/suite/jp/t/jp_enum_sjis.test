#
# Test ENUM values with Japanese characters in sjis encoding
#

SET NAMES sjis;
SET character_set_database = sjis;

CREATE TABLE `ÇsÇP` (`ÇbÇP` ENUM('±','≤','≥'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = innodb;
CREATE TABLE `ÇsÇQ` (`ÇbÇP` ENUM('Ç†','Ç¢','Ç§'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = innodb;
CREATE TABLE `ÇsÇR` (`ÇbÇP` ENUM('É\','è\','ï\'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = innodb;
CREATE TABLE `ÇsÇS` (`ÇbÇP` ENUM('±','≤','≥'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = myisam;
CREATE TABLE `ÇsÇT` (`ÇbÇP` ENUM('Ç†','Ç¢','Ç§'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = myisam;
CREATE TABLE `ÇsÇU` (`ÇbÇP` ENUM('É\','è\','ï\'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = myisam;
CREATE TABLE `ÇsÇV` (`ÇbÇP` ENUM('±','≤','≥'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = MEMORY;
CREATE TABLE `ÇsÇW` (`ÇbÇP` ENUM('Ç†','Ç¢','Ç§'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = MEMORY;
CREATE TABLE `ÇsÇX` (`ÇbÇP` ENUM('É\','è\','ï\'), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = MEMORY;

INSERT INTO `ÇsÇP` VALUES ('±'),('≤'),('≥');
INSERT INTO `ÇsÇQ` VALUES ('Ç†'),('Ç¢'),('Ç§');
INSERT INTO `ÇsÇR` VALUES ('É\'),('è\'),('ï\');
INSERT INTO `ÇsÇS` VALUES ('±'),('≤'),('≥');
INSERT INTO `ÇsÇT` VALUES ('Ç†'),('Ç¢'),('Ç§');
INSERT INTO `ÇsÇU` VALUES ('É\'),('è\'),('ï\');
INSERT INTO `ÇsÇV` VALUES ('±'),('≤'),('≥');
INSERT INTO `ÇsÇW` VALUES ('Ç†'),('Ç¢'),('Ç§');
INSERT INTO `ÇsÇX` VALUES ('É\'),('è\'),('ï\');

