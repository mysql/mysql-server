#
# Test LIKE pattern matching using Japanese characters in ujis encoding
#

SET NAMES ujis;
SET character_set_database = ujis;

CREATE TABLE `г╘г▒` (`г├г▒` char(20), INDEX(`г├г▒`)) DEFAULT CHARSET = ujis engine = innodb;
CREATE TABLE `г╘г▓` (`г├г▒` char(20), INDEX(`г├г▒`)) DEFAULT CHARSET = ujis engine = innodb;
CREATE TABLE `г╘г│` (`г├г▒` char(20), INDEX(`г├г▒`)) DEFAULT CHARSET = ujis engine = innodb;
