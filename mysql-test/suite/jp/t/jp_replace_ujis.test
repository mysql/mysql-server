#
# Test REPLACE() function with Japanese characters in ujis encoding
#

SET NAMES ujis;
SET character_set_database = ujis;

CREATE TABLE `£Τ£±` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = innodb;
CREATE TABLE `£Τ£²` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = innodb;
CREATE TABLE `£Τ£³` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = innodb;
CREATE TABLE `£Τ£΄` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = myisam;
CREATE TABLE `£Τ£µ` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = myisam;
CREATE TABLE `£Τ£¶` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = myisam;
CREATE TABLE `£Τ£·` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = MEMORY;
CREATE TABLE `£Τ£Έ` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = MEMORY;
CREATE TABLE `£Τ£Ή` (`£Γ£±` char(5)) DEFAULT CHARSET = ujis engine = MEMORY;

