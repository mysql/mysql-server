#
# Test LTRIM() function with Japanese characters in sjis encoding
#

SET NAMES sjis;
SET character_set_database = sjis;

CREATE TABLE `ÇsÇP` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = innodb;
CREATE TABLE `ÇsÇQ` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = innodb;
CREATE TABLE `ÇsÇR` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = innodb;
CREATE TABLE `ÇsÇS` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = myisam;
CREATE TABLE `ÇsÇT` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = myisam;
CREATE TABLE `ÇsÇU` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = myisam;
CREATE TABLE `ÇsÇV` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = MEMORY;
CREATE TABLE `ÇsÇW` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = MEMORY;
CREATE TABLE `ÇsÇX` (`ÇbÇP` char(8), INDEX(`ÇbÇP`)) DEFAULT CHARSET = sjis engine = MEMORY;

