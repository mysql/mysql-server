  --replace_column 1 # 2 # 3 # 4 # 5 # 7 # 8 # 9 # 10 #
  EXPLAIN SELECT r, dp FROM t1 WHERE r > 0 or dp > 0;
  --sorted_result
  SELECT r, dp FROM t1 WHERE r > 0 or dp > 0;

  DROP TABLE t1;
}

