}
if (!$mysql_errname)
{
  --replace_column 6 # 7 # 10 # 11 #
  SHOW INDEX IN t1;
  
  INSERT INTO t1 (f,r,d,dp) VALUES
  (1.2345,1422.22,1.2345,1234567.89),
