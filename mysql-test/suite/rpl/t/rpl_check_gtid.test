--echo ==== Part 1 ====

--disable_warnings
--source include/rpl_reset.inc
--enable_warnings

--let $i=1
while ($i != 5)
{
  --echo ---- i=$i ----

  #  1.1 - Generates a set of transactions on the master.
  --connection master
  BEGIN;
  INSERT INTO t1 VALUES(1);
  INSERT INTO t1 VALUES(2);
  COMMIT;

  # Check property 1.1 and 2.1.
  --let $binlog= binlog
  --let $server_uuid= $master_uuid
  if ($i == 1)
  {
    --let $gtid_set_ini= 0
    --let $gtid_set_end= 0
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  if ($i != 1)
  {
    --let $gtid_set_ini= 1
    --let $gtid_set_end= `SELECT $i - 1`
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  --source extra/rpl_tests/rpl_check_gtid.inc

  #  1.2 - Makes the slave to retrieve and apply these transactions.
  --sync_slave_with_master
  --connection slave

  # Check property 1.2 and 2.1.
  --let $binlog= binlog
  --let $server_uuid= $master_uuid
  if ($i == 1)
  {
    --let $gtid_set_ini= 0
    --let $gtid_set_end= 0
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  if ($i != 1)
  {
    --let $gtid_set_ini= 1
    --let $gtid_set_end= `SELECT $i - 1`
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  --source extra/rpl_tests/rpl_check_gtid.inc
  
  # Check property 1.3 and 2.2.
  --let $binlog= relaylog
  --let $server_uuid= $master_uuid
  if ($i == 1)
  {
    --let $gtid_set_ini= 0
    --let $gtid_set_end= 0
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  if ($i != 1)
  {
    --let $gtid_set_ini= 1
    --let $gtid_set_end= `SELECT $i - 1`
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  --source extra/rpl_tests/rpl_check_gtid.inc

  BEGIN;
  INSERT INTO t1 VALUES(1);
  INSERT INTO t1 VALUES(2);
  COMMIT;

  # Check property 1.1 and 2.1.
  --let $binlog= binlog
  --let $server_uuid= $slave_uuid
  if ($i == 1)
  {
    --let $gtid_set_ini= 0
    --let $gtid_set_end= 0
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  if ($i != 1)
  {
    --let $gtid_set_ini= 1
    --let $gtid_set_end= `SELECT $i - 1`
    --let $gtid_ini= `SELECT $i`
    --let $gtid_end= `SELECT $i`
  }
  --source extra/rpl_tests/rpl_check_gtid.inc

  #  1.3 - Rotates both the binary logs on the slave and master.
  FLUSH LOGS;
  --connection master
  FLUSH LOGS;

  inc $i;
}

