!include include/default_mysqld.cnf

[mysqld.1]
log-slave-updates
new=1
innodb
innodb_flush_log_at_trx_commit= 0

[mysqld.2]
log-slave-updates
new=1
innodb
innodb_flush_log_at_trx_commit= 0

[mysqld.3]
log-slave-updates
new=1
innodb
innodb_flush_log_at_trx_commit= 0

[ENV]
SERVER_MYPORT_1= @mysqld.1.port
SERVER_MYPORT_2= @mysqld.2.port
SERVER_MYPORT_3= @mysqld.3.port
