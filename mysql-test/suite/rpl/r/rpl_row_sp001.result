CREATE TABLE test.t2 (a INT NOT NULL AUTO_INCREMENT, t CHAR(4), PRIMARY KEY(a));
CREATE PROCEDURE test.p2(n int)
begin
CASE n
WHEN 1 THEN
UPDATE test.t2 set t ='Tex';
WHEN 2 THEN
UPDATE test.t2 set t ='SQL';
ELSE
UPDATE test.t2 set t ='NONE';
END CASE;
end//
