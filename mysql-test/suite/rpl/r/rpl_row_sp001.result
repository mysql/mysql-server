create table test.t1 (n MEDIUMINT NOT NULL AUTO_INCREMENT, f FLOAT, d DATETIME, PRIMARY KEY(n));
create procedure test.p1()
begin
INSERT INTO test.t1 (f,d) VALUES (RAND(),NOW());
end//

