pattern_database VARCHAR(20),
enabled ENUM('YES', 'NO') CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NOT NULL
DEFAULT 'YES',
replacement VARCHAR(1000),
message VARCHAR(100),
pattern VARCHAR(1000)
);
