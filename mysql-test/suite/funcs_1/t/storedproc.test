BEGIN
    declare cursor1 cursor for SELECT f1 from tb1;
    declare x char;
END//
delimiter ;//

--disable_warnings ONCE
DROP PROCEDURE IF EXISTS sp6;

delimiter //;
--error ER_SP_VARCOND_AFTER_CURSHNDLR
