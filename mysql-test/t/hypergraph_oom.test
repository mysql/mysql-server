CREATE TABLE t12 ( b INTEGER, pk INTEGER, c INTEGER, a INTEGER, KEY (pk), KEY (b), KEY (a) );
CREATE TABLE t13 ( pk INTEGER, a INTEGER, KEY (pk), KEY (a) );

