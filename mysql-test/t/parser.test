UNION
SELECT a FROM t1 ORDER BY a
UNION
SELECT a FROM t1;

(SELECT SQL_CALC_FOUND_ROWS a FROM t1 LIMIT 2)
UNION
(SELECT a FROM t2 ORDER BY a) LIMIT 2;

