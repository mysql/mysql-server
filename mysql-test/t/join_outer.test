SELECT big.*, dt.*
FROM big LEFT JOIN (SELECT id as dt_id,
                           id IS NULL AS nul,
                           id IS NOT NULL AS nnul,
                           id IS TRUE AS t,
                           id IS NOT TRUE AS nt,
                           id IS FALSE AS f,
                           id IS NOT FALSE AS nf,
                           id IS UNKNOWN AS u,
                           id IS NOT UNKNOWN AS nu
                    FROM small) AS dt
     ON big.id=dt.dt_id;

--echo # Check comparison predicates

