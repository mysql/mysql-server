  (NULL, 2, 3+1), (1, NULL, 3+1), (1, 2+1, NULL),
  (NULL, 2, 3-1), (1, NULL, 3-1), (1, 2-1, NULL);

SELECT (1,2,3) = (1,   NULL, 3);
SELECT (1,2,3) = (1+1, NULL, 3);
