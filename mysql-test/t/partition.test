--echo #
create table t1 (
  a int not null,
  b int not null,
  pk int not null,
  primary key (pk),
  key(a),
  key(b)
) partition by hash(pk) partitions 10;

insert into t1 values (1,2,4); # both
insert into t1 values (1,0,17);  # left
insert into t1 values (1,2,25);   # both

