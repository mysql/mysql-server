# Tested with locking
#

CREATE TABLE t1 (
  cid mediumint(9) DEFAULT '0' NOT NULL auto_increment,
  firstname varchar(32) DEFAULT '' NOT NULL,
  surname varchar(32) DEFAULT '' NOT NULL,
  PRIMARY KEY (cid)
);
