            WHERE f1 > 3 and f2 < 50 and i0 > 3;
--replace_regex /cost=\d+.\d+/cost=***/  /rows=\d+/rows=***/
--skip_if_hypergraph  # Depends on the query plan.
eval EXPLAIN FORMAT=tree $query;
eval $query;

