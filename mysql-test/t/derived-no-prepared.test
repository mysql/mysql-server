ANALYZE TABLE t1, t2;

SELECT field1
FROM (SELECT alias1.c5 AS field1,
             alias1.c13 AS field2,
             alias1.c1 AS field4
      FROM t1 AS alias1 RIGHT JOIN t2 AS alias2
           ON alias1.pk = alias2.c12
      ORDER BY field2
     ) as dt;

SELECT field1
FROM (SELECT *
      FROM (SELECT alias1.c5 AS field1,
                   alias1.c13 AS field2,
                   alias1.c1 AS field4
            FROM t1 AS alias1 RIGHT JOIN t2 AS alias2
                 ON alias1.pk = alias2.c12
            ORDER BY field2
           ) AS dt_inner
     ) AS dt_outer;

DROP TABLE t1, t2;
