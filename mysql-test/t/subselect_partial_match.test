--echo -------------------------------------------------------------------------

create table t1 (a1 char(2), a2 char(1), a3 char(1) not null, a4 char(10));
create table t2 (b1 char(2), b2 char(1), b3 char(1),          b4 char(1));

insert into t1 values ('99',  NULL, 'j', 'f');

