SET NAMES 'filename';
SET NAMES 'utf8';
--error ER_UNKNOWN_CHARACTER_SET
SET CHARACTER_SET_CLIENT=17;
SET CHARACTER_SET_CLIENT=33;

--disable_warnings
DROP DATABASE IF EXISTS db21958734;
