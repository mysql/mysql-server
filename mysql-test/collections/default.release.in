#This collection is used for all platforms on release clones
#and for gcov/devbld platforms on all other trees.
#You may find default.release and default.release.done files under
#collections. Please do not modify these files since they are
#are generated by Cmake.

# It is possible to include other collections in this file
# with something like the below (remove space after #):

# include default.daily
# include default.weekly

perl mysql-test-run.pl --force --timer --parallel=auto --comment=debug      --vardir=var-debug --skip-rpl --report-features --debug-server
# Run with --non-parallel-test option so that non parallel tests are also run
perl mysql-test-run.pl --force --timer --parallel=auto --comment=normal     --vardir=var-normal --report-features --unit-tests-report --non-parallel-test
perl mysql-test-run.pl --force --timer --parallel=auto --comment=ps         --vardir=var-ps --ps-protocol
perl mysql-test-run.pl --force --timer --parallel=auto --comment=funcs2     --vardir=var-funcs2     --suite=funcs_2
perl mysql-test-run.pl --force --timer --parallel=auto --comment=partitions --vardir=var-parts      --suite=parts
perl mysql-test-run.pl --force --timer --parallel=auto --comment=stress     --vardir=var-stress     --suite=stress
perl mysql-test-run.pl --force --timer --parallel=auto --comment=jp         --vardir=var-jp         --suite=jp
perl mysql-test-run.pl --force --timer --parallel=auto --comment=nist       --vardir=var-nist       --suite=nist
perl mysql-test-run.pl --force --timer --parallel=auto --comment=nist+ps    --vardir=var-nist_ps    --suite=nist     --ps-protocol
perl mysql-test-run.pl --timer --force --comment=memcached --vardir=var-memcached --parallel=auto --suite=memcached
perl mysql-test-run.pl --force --timer  --testcase-timeout=60 --parallel=auto --comment=interactive_tests  --vardir=var-interactive  --suite=interactive_utilities

# Group Replication
perl mysql-test-run.pl --timer --force --parallel=6 --comment=group_replication --vardir=var-group_replication --suite=group_replication
