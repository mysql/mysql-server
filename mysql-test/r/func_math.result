CREATE PROCEDURE test_round(in arg bigint)
BEGIN
DECLARE i int;
SET i = 0;
WHILE (i >= -20) DO
BEGIN
# Ignore SQLSTATE 22003 numeric value out of range
DECLARE CONTINUE HANDLER FOR SQLSTATE '22003' SHOW ERRORS;
SELECT arg, i, round(arg, i);
END;
SET i = i - 1;
END WHILE;
END//
