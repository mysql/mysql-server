CREATE PROCEDURE my_proc() BEGIN
SET @@session.autocommit=OFF;
SET @@session.time_zone='-6:00';
SET @@session.tx_isolation='READ-COMMITTED';
END;|
CALL my_proc;
-- tx_isolation
-- READ-COMMITTED
-- autocommit
-- OFF
-- time_zone
-- -06:00

CALL my_proc;
-- tx_isolation
-- READ-COMMITTED
-- autocommit
-- OFF
-- time_zone
-- -06:00

DROP PROCEDURE my_proc;
