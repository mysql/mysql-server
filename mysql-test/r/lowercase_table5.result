CREATE TABLE t1(a INT);
# t1 should inherit utf8_unicode_ci
SHOW CREATE TABLE t1;
Table	Create Table
t1	CREATE TABLE `t1` (
  `a` int(11) DEFAULT NULL
) ENGINE=ENGINE DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci
DROP TABLE t1;
