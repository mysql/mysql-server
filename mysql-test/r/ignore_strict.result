FOR EACH ROW BEGIN
INSERT IGNORE INTO t3 VALUES (5),(6);
END|
CREATE TRIGGER post_update_t1 AFTER UPDATE ON t1
FOR EACH ROW BEGIN
INSERT IGNORE INTO t3 VALUES(4);
END|
INSERT INTO t1 VALUES(2);
ERROR 23000: Duplicate entry '2' for key 'PRIMARY'
SELECT * FROM t3;
c
