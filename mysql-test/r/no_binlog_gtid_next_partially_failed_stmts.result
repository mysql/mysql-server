#
include/assert.inc [Committed gtid MASTER_UUID:2 into @@GLOBAL.GTID_EXECUTED]
SET SESSION GTID_NEXT='AUTOMATIC';
FLUSH LOGS;
include/assert.inc [Saved gtid MASTER_UUID:2 into mysql.gtid_executed table]
