# 4. Names collate differently. On trunk, the queries below returns
#    the same result set.
SELECT count(*) = IF(@@lower_case_table_names = 0, 4, 9)
FROM information_schema.tables
WHERE table_name LIKE 'TAB%';
count(*) = IF(@@lower_case_table_names = 0, 4, 9)
1
SELECT count(*) = IF(@@lower_case_table_names = 0, 5, 9)
FROM information_schema.tables
WHERE table_name LIKE 'tab%';
