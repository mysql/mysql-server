            "key": "k1",
            "used_key_parts": [
              "a"
            ],
            "key_length": "5",
            "rows_examined_per_scan": 4,
            "rows_produced_per_join": 256,
            "filtered": "100.00",
            "cost_info": {
              "read_cost": "1.75",
              "eval_cost": "25.60",
              "prefix_cost": "27.35",
              "data_read_per_join": "260K"
            },
            "used_columns": [
              "pk",
              "a",
              "b",
              "c"
            ],
            "attached_condition": "(`test`.`t1`.`b` is not null)"
          }
        },
        {
          "table": {
            "table_name": "t2",
            "access_type": "eq_ref",
            "possible_keys": [
              "PRIMARY"
            ],
            "key": "PRIMARY",
            "used_key_parts": [
              "d"
            ],
            "key_length": "4",
            "ref": [
              "test.t1.b"
            ],
