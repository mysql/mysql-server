JSON_VALID( CAST(CAST('2015-01-15' AS DATE) as CHAR CHARACTER SET 'utf8') )
0
# OK, though:
select JSON_VALID( CAST(CURDATE() as JSON) );
JSON_VALID( CAST(CURDATE() as JSON) )
1

