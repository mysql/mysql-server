CREATE TABLE t1 (
pk INTEGER,
PRIMARY KEY (pk)
);
INSERT INTO t1 VALUES (1), (2);
CREATE VIEW v_t1 AS SELECT * FROM t1;
SELECT pk
FROM t1
WHERE
pk > 8
OR ((pk BETWEEN 9 AND 13) AND pk = 90)
;
pk
SELECT pk
FROM v_t1
WHERE
pk > 8
OR ((pk BETWEEN 9 AND 13) AND pk = 90)
;
pk
DROP VIEW v_t1;
DROP TABLE t1;
