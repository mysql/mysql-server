UPDATE t1 SET b = 999 WHERE c = 300;
SELECT * FROM t1;
a	b	c
99	NULL	3
99	NULL	3
99	NULL	30
199	NULL	300

DROP TRIGGER t1_bi;
DROP TRIGGER t1_bu;
