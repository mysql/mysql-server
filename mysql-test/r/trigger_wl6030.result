###################################################################
CREATE TABLE t1(a INT, b INT NOT NULL);

# - Unconditional SET in a trigger.

CREATE TRIGGER t1_bi BEFORE INSERT ON t1 FOR EACH ROW
SET NEW.b = NULL;

INSERT INTO t1(a) VALUES (1);
