CREATE PROCEDURE create_table_and_insert_rows()
BEGIN
DECLARE count INT;
SET count = 1;
SELECT SLEEP(10);
CREATE TABLE t1 (f1 INT);
INSERT INTO t1 VALUES (7894);
WHILE count <= 5 DO
INSERT INTO t1 SELECT * FROM t1;
SET count = count + 1;
END WHILE;
SET count = 1;
WHILE count <= 10000 DO
SELECT count(*) from t1;
SET count = count + 1;
END WHILE;
END
$
