#---------------- Index merge test 1 -------------------------------------------
# (Start of test file: index_merge1.inc)
SET SESSION DEFAULT_STORAGE_ENGINE = MyISAM;
SET SESSION EXPLAIN_FORMAT = tree;
drop table if exists t0, t1, t2, t3, t4;
# Create and fill a table with simple keys
create table t0
(
key1 int not null,
key2 int not null,
key3 int not null,
key4 int not null,
key5 int not null,
key6 int not null,
key7 int not null,
key8 int not null,
INDEX i1(key1),
INDEX i2(key2),
INDEX i3(key3),
INDEX i4(key4),
INDEX i5(key5),
INDEX i6(key6),
INDEX i7(key7),
INDEX i8(key8)
);
create procedure populate_t0()
begin
declare d int default 2;
insert into t0 values (1,1,1,1,1,1,1,1023),(2,2,2,2,2,2,2,1022);
while d<=POW(2,9) do
insert into t0 select key1+d, key2+d, key3+d, key4+d, key5+d,
key6+d, key7+d, key8-d from t0;
set d = d*2;
end while;
end|
call populate_t0();
drop procedure populate_t0;
analyze table t0;
Table	Op	Msg_type	Msg_text
