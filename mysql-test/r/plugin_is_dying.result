SHOW PLUGINS;
SELECT PLUGIN_NAME, PLUGIN_STATUS, LOAD_OPTION from INFORMATION_SCHEMA.PLUGINS
WHERE PLUGIN_NAME like 'pfs_example%';
PLUGIN_NAME	PLUGIN_STATUS	LOAD_OPTION
SET DEBUG_SYNC='in_plugin_initialize WAIT_FOR go_init';
"con1 installing plugin ..."
INSTALL PLUGIN pfs_example_plugin_employee SONAME 'pfs_example_plugin_employee.so';
SHOW PLUGINS;
SELECT PLUGIN_NAME, PLUGIN_STATUS, LOAD_OPTION from INFORMATION_SCHEMA.PLUGINS
WHERE PLUGIN_NAME like 'pfs_example%';
PLUGIN_NAME	PLUGIN_STATUS	LOAD_OPTION
pfs_example_plugin_employee	INACTIVE	ON
SET DEBUG_SYNC='now SIGNAL go_init';
SHOW PLUGINS;
SELECT PLUGIN_NAME, PLUGIN_STATUS, LOAD_OPTION from INFORMATION_SCHEMA.PLUGINS
WHERE PLUGIN_NAME like 'pfs_example%';
PLUGIN_NAME	PLUGIN_STATUS	LOAD_OPTION
pfs_example_plugin_employee	ACTIVE	ON
SET DEBUG_SYNC='in_plugin_check_uninstall WAIT_FOR go_deinit';
UNINSTALL PLUGIN pfs_example_plugin_employee;;
SHOW PLUGINS;
SELECT PLUGIN_NAME, PLUGIN_STATUS, LOAD_OPTION from INFORMATION_SCHEMA.PLUGINS
WHERE PLUGIN_NAME like 'pfs_example%';
PLUGIN_NAME	PLUGIN_STATUS	LOAD_OPTION
pfs_example_plugin_employee	DELETING	ON
SET DEBUG_SYNC='now SIGNAL go_deinit';
SHOW PLUGINS;
SELECT PLUGIN_NAME, PLUGIN_STATUS, LOAD_OPTION from INFORMATION_SCHEMA.PLUGINS
WHERE PLUGIN_NAME like 'pfs_example%';
PLUGIN_NAME	PLUGIN_STATUS	LOAD_OPTION
