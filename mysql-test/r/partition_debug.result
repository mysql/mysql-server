f2 iNT , f3 INT,
KEY `IDX_F2` (f2),
KEY `IDX_F3` (f3)
) ENGINE=INNODB
PARTITION BY RANGE (f1) (
PARTITION p0 VALUES LESS THAN (10),
PARTITION p1 VALUES LESS THAN (20));
CREATE TABLE t1_normal_table ( f1 INT PRIMARY KEY,
f2 iNT , f3 INT,
KEY `IDX_F3` (f3),
KEY `IDX_F2` (f2)
) ENGINE=INNODB;
