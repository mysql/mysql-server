source include/have_tokudb.inc;
# verify that truncate table leaves the table empty
--disable_warnings
drop table if exists t;
--enable_warnings
create table t (a int, b int, primary key (a)) engine=tokudb;
insert into t values (1,2),(2,4),(3,8);
select * from t;
truncate table t;
select * from t;
drop table t;
