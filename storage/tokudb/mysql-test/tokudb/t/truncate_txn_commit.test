source include/have_tokudb.inc;
# test truncate commit
set autocommit=off;
--disable_warnings
drop table if exists t;
--enable_warnings
create table t (a int, b int, primary key (a)) engine=tokudb;
start transaction;
insert into t values (1,2),(2,4),(3,8);
commit;
start transaction;
select * from t;
commit;
start transaction;
truncate table t;
commit;
start transaction;
select * from t;
commit;
drop table t;