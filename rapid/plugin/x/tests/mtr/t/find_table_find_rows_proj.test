Mysqlx.Crud.Find {
  collection {
    name: "city"
    schema: "xtest"
  }
  data_model: TABLE
  projection {
    source {
      type: IDENT
      identifier {
        name: "Name"
      }
    }
  }
  projection {
    source {
      type: IDENT
      identifier {
        name: "Population"
      }
    }
    alias: "Pop"
  }
  criteria {
    type: OPERATOR
    operator {
      name: "<"
      param {
        type: IDENT
        identifier {
          name: "Population"
        }
      }
      param {
        type: LITERAL
        literal {
          type: V_SINT
          v_signed_int: 150000
        }
      }
    }
  }
}

