## Preamble
--echo ================================================================================
--echo PREAMBLE
--echo ================================================================================
--source ../include/xplugin_preamble.inc
create user insertcruduser@localhost identified by 'insertcruduser';
grant all on *.* to insertcruduser@localhost;

