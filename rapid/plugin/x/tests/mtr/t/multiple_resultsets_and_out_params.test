CALL update_and_out_param_and_select(@max_val);
SELECT @max_val;

CALL insert_no_select();
CALL insert_and_select();
CALL insert_and_out_param(@max_val);
SELECT @max_val;
CALL insert_and_out_param_and_select(@max_val);
SELECT @max_val;

