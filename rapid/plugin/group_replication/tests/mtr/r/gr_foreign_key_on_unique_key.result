CREATE TABLE t1 (a INT,b INT UNIQUE NOT NULL, PRIMARY KEY(a));
CREATE TABLE t2 (c1 INT PRIMARY KEY,c2 INT,FOREIGN KEY(c2) REFERENCES t1(b));

