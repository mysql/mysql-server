[connection server1]

CREATE TABLE t1 (a INT PRIMARY KEY);

CREATE PROCEDURE insert_into_t1(IN a INT, IN b INT)
BEGIN
DECLARE x INT;
SET x=a;
WHILE x<b DO
INSERT INTO t1 VALUES (x);
SET x=x+1;
END WHILE;
END$$

CREATE FUNCTION f1() RETURNS INT RETURN (SELECT COUNT(*) FROM t1);

Calling procedure
CALL insert_into_t1(0,10);

SELECT f1();
f1()
10
